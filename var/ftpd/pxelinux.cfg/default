DEFAULT vesamenu.c32
#PROMPT 1
PROMPT 0
#DISPLAY message.txt
MENU TITLE Main Menu
timeout 100

#Menus

LABEL Ubuntu LiveCD 11.04.3
    menu label Ubuntu LiveCD 11.04.3
    kernel ubuntu/casper/vmlinuz
    APPEND initrd=ubuntu/casper/initrd.lz netboot=nfs root=/dev/nfs nfsroot=10.12.30.1:/var/ftpd ip=dhcp

LABEL mfsBSD
    menu label MfsBSD 8.2
    kernel memdisk
    initrd mfsbsd/mfsbsd.iso
    append iso

LABEL DOS
    kernel memdisk
    append initrd=fdboot/fdboot.img

LABEL Memtest
kernel mt86plus

